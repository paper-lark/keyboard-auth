{
  "name": "keyboard-auth-server",
  "version": "0.0.0",
  "description": "Keyboard authenticator server",
  "main": "index.ts",
  "scripts": {
    "start:dev": "env TS_NODE_FILES=true ts-node --project tsconfig.json src/index.ts",
    "test": "jest -c config/jest.js",
    "lint": "prettier-tslint fix '**/*.ts'",
    "create:db": "docker run -d --name project_pg -p 12001:5432 postgres:11 && docker exec -it project_pg psql -U postgres -c 'create database project'",
    "stop:db": "docker stop project_pg",
    "start:db": "docker start project_pg",
    "connect:db": "pgcli --host=127.0.0.1 --port=12001 --dbname=project --username=postgres"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paper-lark/keyboard-auth.git"
  },
  "keywords": [
    "keyboard"
  ],
  "author": "Max Zhuravsky <paperlark@yandex.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/paper-lark/keyboard-auth/issues"
  },
  "homepage": "https://github.com/paper-lark/keyboard-auth#readme",
  "dependencies": {
    "google-protobuf": "3.10.0-rc.1",
    "grpc": "1.23.3",
    "grpc-error": "1.0.0",
    "grpc-ts-middleware": "1.0.8",
    "guid-typescript": "1.0.9",
    "lodash": "4.17.15",
    "mem": ">=4.0.0",
    "moment": "2.24.0",
    "pg": "7.12.1",
    "reflect-metadata": "0.1.13",
    "typeorm": "0.2.19",
    "typescript": "3.6.3",
    "winston": "3.2.1"
  },
  "devDependencies": {
    "@types/google-protobuf": "3.7.1",
    "@types/grpc-error": "1.0.0",
    "@types/jest": "24.0.18",
    "@types/lodash": "4.14.138",
    "@types/moment": "2.13.0",
    "@types/node": "12.7.5",
    "grpc-tools": "1.8.0",
    "grpc_tools_node_protoc_ts": "2.5.4",
    "jest": "24.9.0",
    "prettier": "1.18.2",
    "prettier-tslint": "0.4.2",
    "ts-jest": "24.1.0",
    "ts-node": "8.3.0",
    "tslint": "5.20.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-config-security": "1.16.0"
  }
}
